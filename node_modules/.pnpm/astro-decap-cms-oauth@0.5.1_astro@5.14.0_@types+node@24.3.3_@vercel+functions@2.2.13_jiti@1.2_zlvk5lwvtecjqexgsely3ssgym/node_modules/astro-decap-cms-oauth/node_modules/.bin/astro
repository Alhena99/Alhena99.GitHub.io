#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/1/Twilight/node_modules/.pnpm/astro@5.14.0_@types+node@24.3.3_@vercel+functions@2.2.13_jiti@1.21.7_lightningcss@1.29.3_roll_y65advndblbzfcfcvu4de7pcpa/node_modules/astro/node_modules:/mnt/d/1/Twilight/node_modules/.pnpm/astro@5.14.0_@types+node@24.3.3_@vercel+functions@2.2.13_jiti@1.21.7_lightningcss@1.29.3_roll_y65advndblbzfcfcvu4de7pcpa/node_modules:/mnt/d/1/Twilight/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/1/Twilight/node_modules/.pnpm/astro@5.14.0_@types+node@24.3.3_@vercel+functions@2.2.13_jiti@1.21.7_lightningcss@1.29.3_roll_y65advndblbzfcfcvu4de7pcpa/node_modules/astro/node_modules:/mnt/d/1/Twilight/node_modules/.pnpm/astro@5.14.0_@types+node@24.3.3_@vercel+functions@2.2.13_jiti@1.21.7_lightningcss@1.29.3_roll_y65advndblbzfcfcvu4de7pcpa/node_modules:/mnt/d/1/Twilight/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../astro@5.14.0_@types+node@24.3.3_@vercel+functions@2.2.13_jiti@1.21.7_lightningcss@1.29.3_roll_y65advndblbzfcfcvu4de7pcpa/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../astro@5.14.0_@types+node@24.3.3_@vercel+functions@2.2.13_jiti@1.21.7_lightningcss@1.29.3_roll_y65advndblbzfcfcvu4de7pcpa/node_modules/astro/astro.js" "$@"
fi
